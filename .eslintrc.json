{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true,
    "node": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "semi": ["error", "always"]
  },
  "globals": {
    "moment": true,
    "stateFlagDetail":true,
    "L":true,
    "google":true
}
}
